<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_F4F4F4"
    android:orientation="vertical">

    <include layout="@layout/include_title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:background="@drawable/bg_ffffff_7_top_left">

                <RelativeLayout
                    android:id="@+id/rl_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tv_name_hint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:text="@string/txt_device_name"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:id="@+id/tv_device_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:layout_toEndOf="@+id/tv_name_hint"
                        android:gravity="end"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13"
                        tools:text="@string/txt_device_name" />

                    <EditText
                        android:id="@+id/edt_device_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_30"
                        android:layout_marginTop="@dimen/dp_7"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_toEndOf="@+id/tv_name_hint"
                        android:background="@drawable/bg_f8f8f8_5"
                        android:gravity="center_vertical|end"
                        android:hint="@string/txt_input_device_name"
                        android:paddingStart="@dimen/dp_10"
                        android:paddingEnd="@dimen/dp_10"
                        android:textColor="@color/color_333333"
                        android:textColorHint="@color/color_999999"
                        android:textSize="@dimen/sp_13"
                        android:visibility="gone" />
                </RelativeLayout>

                <View
                    android:id="@+id/view_device_name"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/rl_name"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <TextView
                    android:id="@+id/tv_account_hint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_name"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_device_account"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_copy_account"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_name"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:layout_toEndOf="@+id/tv_account_hint"
                    android:text="@string/txt_long_copy"
                    android:textColor="@color/color_2E7BEC"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_device_account"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_name"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:layout_toEndOf="@+id/tv_copy_account"
                    android:gravity="end"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    tools:text="@string/txt_device_account" />

                <View
                    android:id="@+id/view_device_account"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_device_account"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_account"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_device_type"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_device_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_account"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    tools:text="@string/txt_device_type" />

                <View
                    android:id="@+id/view_device_type"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_device_type"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_type"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_device_status"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_device_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_type"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:textColor="@color/color_F22E13"
                    android:textSize="@dimen/sp_13"
                    tools:text="@string/txt_device_status" />

                <View
                    android:id="@+id/view_device_status"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_device_status"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_status"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_location_time"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_location_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_status"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:visibility="gone"
                    tools:text="@string/txt_location_time" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/color_FFFFFF">

                <TextView
                    android:id="@+id/tv_iccid_hint"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_iccid"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_copy_iccid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:layout_toEndOf="@+id/tv_iccid_hint"
                    android:text="@string/txt_long_copy"
                    android:textColor="@color/color_2E7BEC"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_device_iccid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:layout_toEndOf="@+id/tv_copy_iccid"
                    android:gravity="end"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    tools:text="@string/txt_iccid_number" />

                <View
                    android:id="@+id/view_device_iccid"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_device_iccid"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_iccid"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_lat_lon"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_lat_lon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_device_iccid"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    tools:text="@string/txt_lat_lon" />

                <View
                    android:id="@+id/view_lat_lon"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_lat_lon"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_lat_lon"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_opening_time"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv_opening_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_lat_lon"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    tools:text="@string/txt_opening_time" />

                <View
                    android:id="@+id/view_opening_time"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/tv_opening_time"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_opening_time"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:text="@string/txt_expire_date"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_expire_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_opening_time"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:layout_marginBottom="@dimen/dp_15"
                    android:textColor="@color/color_333333"
                    android:textSize="@dimen/sp_13"
                    android:visibility="gone"
                    tools:text="@string/txt_expire_date" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginBottom="@dimen/dp_10"
                android:background="@drawable/bg_ffffff_7_bottom_right">

                <RelativeLayout
                    android:id="@+id/rl_contact_person"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tv_contact_person_hint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:text="@string/txt_contact_person"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:id="@+id/tv_contact_person"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:layout_toEndOf="@+id/tv_contact_person_hint"
                        android:gravity="end"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13"
                        tools:text="@string/txt_contact_person" />

                    <EditText
                        android:id="@+id/edt_contact_person"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_30"
                        android:layout_marginTop="@dimen/dp_7"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_toEndOf="@+id/tv_contact_person_hint"
                        android:background="@drawable/bg_f8f8f8_5"
                        android:gravity="center_vertical|end"
                        android:hint="@string/txt_input_contact_person"
                        android:paddingStart="@dimen/dp_10"
                        android:paddingEnd="@dimen/dp_10"
                        android:textColor="@color/color_333333"
                        android:textColorHint="@color/color_999999"
                        android:textSize="@dimen/sp_13"
                        android:visibility="gone" />
                </RelativeLayout>

                <View
                    android:id="@+id/view_contact_person"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/rl_contact_person"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <RelativeLayout
                    android:id="@+id/rl_bind_mobile"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_contact_person">

                    <TextView
                        android:id="@+id/tv_bind_mobile_hint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:text="@string/txt_bind_mobile_hint"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:id="@+id/tv_bind_mobile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:layout_toEndOf="@+id/tv_bind_mobile_hint"
                        android:gravity="end"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13"
                        tools:text="@string/txt_bind_mobile_hint" />
                </RelativeLayout>

                <View
                    android:id="@+id/view_backup_number"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_below="@+id/rl_bind_mobile"
                    android:layout_marginStart="@dimen/dp_12"
                    android:layout_marginEnd="@dimen/dp_12"
                    android:background="@color/color_E5E5E5" />

                <RelativeLayout
                    android:id="@+id/rl_backup_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/view_backup_number">

                    <TextView
                        android:id="@+id/tv_backup_number_hint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:text="@string/txt_backup_phone_number"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:id="@+id/tv_backup_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:layout_toEndOf="@+id/tv_backup_number_hint"
                        android:gravity="end"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13"
                        tools:text="@string/txt_backup_phone_number" />

                    <EditText
                        android:id="@+id/edt_backup_number"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_30"
                        android:layout_marginTop="@dimen/dp_7"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_toEndOf="@+id/tv_backup_number_hint"
                        android:background="@drawable/bg_f8f8f8_5"
                        android:gravity="center_vertical|end"
                        android:hint="@string/txt_input_backup_phone_number"
                        android:inputType="number"
                        android:maxLength="20"
                        android:paddingStart="@dimen/dp_10"
                        android:paddingEnd="@dimen/dp_10"
                        android:textColor="@color/color_333333"
                        android:textColorHint="@color/color_999999"
                        android:textSize="@dimen/sp_13"
                        android:visibility="gone" />

                    <View
                        android:id="@+id/view_contact_number"
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:layout_below="@+id/tv_backup_number_hint"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:background="@color/color_E5E5E5" />

                    <TextView
                        android:id="@+id/tv_address_hint"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/view_contact_number"
                        android:layout_marginStart="@dimen/dp_12"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:text="@string/txt_address"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13" />

                    <TextView
                        android:id="@+id/tv_address"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/view_contact_number"
                        android:layout_alignParentEnd="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginEnd="@dimen/dp_12"
                        android:layout_marginBottom="@dimen/dp_15"
                        android:layout_toEndOf="@+id/tv_address_hint"
                        android:gravity="center_vertical|end"
                        android:textColor="@color/color_333333"
                        android:textSize="@dimen/sp_13"
                        tools:text="@string/txt_address" />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>